# DevSecOps Tools - Environment Configuration Example
# Скопируйте этот файл в .env и настройте под свои нужды

# Backend Configuration
PYTHONUNBUFFERED=1
LOG_LEVEL=info

# Ports
BACKEND_PORT=8000
FRONTEND_PORT=3000

# Network
NETWORK_NAME=devsecops-network

# Image Tags
BACKEND_IMAGE_TAG=1.3.0
FRONTEND_IMAGE_TAG=1.3.0
